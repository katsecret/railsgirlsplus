<p id="notice"><%= notice %></p>

<h1>Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Condition1</th>
      <th>Condition2</th>
      <th>Start at</th>
      <th>End at</th>
      <th>Document</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.year %></td>
        <td><%= contract.condition1 %></td>
        <td><%= contract.condition2 %></td>
        <td><%= contract.start_at %></td>
        <td><%= contract.end_at %></td>
        <td><%= contract.supplier.name %></td>
        <td><%= link_to "Document", contract.document_url  %></td>
        <td><%= link_to 'Show', contract %></td>
        <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
        <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract', new_contract_path %>
<br>
<%= link_to 'Suppliers', suppliers_path %>
